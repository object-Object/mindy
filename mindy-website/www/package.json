{
    "name": "mindy-website",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "yarn wasm-build && yarn build-only",
        "build-only": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "wasm": "wasm-pack build .. --target web --dev && yarn upgrade mindy-website",
        "wasm-build": "yarm wasm-build-only && yarn upgrade mindy-website",
        "wasm-build-only": "wasm-pack build .. --target web --release"
    },
    "dependencies": {
        "@mantine/core": "^8.2.4",
        "@mantine/hooks": "^8.2.4",
        "@xyflow/react": "^12.8.3",
        "mindy-website": "file:../pkg",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-icons": "^5.5.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.33.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/react": "^19.1.10",
        "@types/react-dom": "^19.1.7",
        "@vitejs/plugin-react": "^5.0.0",
        "eslint": "^9.33.0",
        "eslint-plugin-react-dom": "^1.52.4",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "eslint-plugin-react-x": "^1.52.4",
        "globals": "^16.3.0",
        "postcss": "^8.5.6",
        "postcss-preset-mantine": "^1.18.0",
        "postcss-simple-vars": "^7.0.1",
        "prettier": "^3.6.2",
        "typescript": "~5.8.3",
